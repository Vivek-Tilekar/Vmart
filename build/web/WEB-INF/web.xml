        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageGet</servlet-name>
        <url-pattern>/ImageGet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cartinfo</servlet-name>
        <url-pattern>/Cartinfo</url-pattern>
