    <servlet-mapping>
        <servlet-name>CartRemove</servlet-name>
        <url-pattern>/CartRemove</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderServlet</servlet-name>
        <url-pattern>/OrderServlet</url-pattern>
    </servlet-mapping>
</web-app>
